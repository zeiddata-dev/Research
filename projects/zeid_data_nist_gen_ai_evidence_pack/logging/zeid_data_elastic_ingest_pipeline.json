{
  "description": "Zeid Data AI telemetry pipeline",
  "processors": [
    {
      "json": {
        "field": "message",
        "target_field": "zeid_ai",
        "add_to_root": false
      }
    },
    {
      "set": {
        "field": "event.dataset",
        "value": "zeid_data.ai.telemetry"
      }
    },
    {
      "date": {
        "field": "zeid_ai.event_time",
        "formats": [
          "ISO8601"
        ],
        "target_field": "@timestamp"
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}